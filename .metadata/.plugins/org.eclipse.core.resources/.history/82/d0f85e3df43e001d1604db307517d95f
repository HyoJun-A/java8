package sec1;

public class Interface2 {
	int MAX = 20;	//상수 필드
	int MIN = 1;
	// = abtract void method1(); 과 같다
	void method1();	//일반 멤버 메소드는 선언만 가능하며, 실행 내용은 기술할 수 없다.
	int method2(int pa1, String pa2);
	static void method3(int pa1){	//정적 메소드는 선언과 실행 내용을 모두 기술
		System.out.println(pa1 + "값을 전달 받았습니다.");
	};
	default int method4(String pa1){	//디폴트 메소드는 선언과 실행 내용을 모두 기술
		return 1004;
	};
}
