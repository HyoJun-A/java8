package sec1;
// 함수적 인터페이스 - 하나의 인터페이스는 하나의 메소드만 정의할 수 있다.
@FunctionalInterface
interface Calc {	//interface하나당 하나의 메소드만 정의할 수 있음
	public void method1();
//	public void method2(int x, int y);
}

@FunctionalInterface
interface Calc2 {	
	public void method2();
}


@FunctionalInterface
interface Calc3 {	
	public void method2(int x,int y);
}

public class Lambda2 {
	public static void main(String[] args) {
		Calc4 max = (x, y) -> x>y ? x : y;
	}
}
