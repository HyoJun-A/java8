package sec1;

public class Parent {
	int filed1;
	String field2;
	
	public Parent(){}
	
	public Parent(int filed1) {
		this(filed1, "ahn");
	}
	
	public Parent(int field1, String field2){
		this.filed1 = field1;
		this.field2 = field2;
	}
	
	public void method1(){}
	public int method2(){
		return filed1 + 100;
	}
}
