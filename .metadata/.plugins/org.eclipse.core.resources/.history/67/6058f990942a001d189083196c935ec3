package sec2;

public class Cast2 {

	public static void main(String[] args) {
		int a = 100, b = 23, c;
		float d, e, f;
		c = a / b;		//4  -> 자동 형 변환
		System.out.println("c="+c);
		d = (float) a / b;		//4.0 -> (float)는 형변환 연산자 : 강제 형 변환
		System.out.println("d="+d);
		e = 500 / 3.0f;			//상수 일경우 숫자에 f를 붙인다, 형 변환 연산
		System.out.println("e="+e);
		
	}

}
