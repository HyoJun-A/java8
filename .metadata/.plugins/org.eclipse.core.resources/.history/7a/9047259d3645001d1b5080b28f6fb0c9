package sec1;
// 함수적 인터페이스
@FunctionalInterface
interface Calc {	//interface하나당 하나의 기능만 구현할 수 있음
	public void method1();
//	public void method2(int x, int y);
}

@FunctionalInterface
interface Calc2 {	
	public void method2(int x,int y);
}


@FunctionalInterface
interface Calc3 {	
	public void method2();
}

public class Lambda2 {
	public static void main(String[] args) {
		
	}
}
