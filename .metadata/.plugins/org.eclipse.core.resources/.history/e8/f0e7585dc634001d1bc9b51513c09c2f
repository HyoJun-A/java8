package sec4;

public class Ex5 {
	public static void main(String[] args) {
		// 4x * 5y = 60 : x, y에 특정 값을 대입하였을 경우에 계산결과가 60이 되는 해
		for(int i = 1; i <= 10; i++){
			for(int j = 1; j <= 10; j++){
				if((4*i) + (5*j) == 60){
					System.out.println("( " + i + ", " + j +" )");
				}
			}
		}
	}

}
