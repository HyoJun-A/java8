package sec1;

public class ChildEx1 {

	public static void main(String[] args) {
		Child c1 = new Child();
		c1.filed1 = 10;		//부모 클래스로 부터 물려 받은 filed1
		c1.field2 = "kim";	//부모 클래스로 부터 물려 받은 filed2
		c1.filed3 = 20;		//자신 클래스에서 새로 부여된 filed3
		
		//자식 클래스는 부모 요소를 모두 활용할 수 있지만, 
		//부모 클래스는 자식 클래스에만 있는 요소를 활용할 수 없다.
		Parent c2 = new Parent();
		c2.filed1 = 100;	//
		c2.field2 = "lee";	//
//		c2.field3 = 200;	//filed3는 자식클래스에 있는 것이므로 활용할 수 없다.
		
		Parent c3;
		c3 = new Child();
		c3.method1();
		c3.method2();
		
		c3 = new Child();
		c3.method3();
		c3.method4();
		
	}

}
