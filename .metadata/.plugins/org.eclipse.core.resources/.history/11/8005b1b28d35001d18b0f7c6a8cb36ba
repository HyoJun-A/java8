package sec1;

public class Type3 {
	public static void main(String[] args) {
		String name = "ahn";
		char[] irum = {'a', 'h', 'n'};
		String nick = "ahn";
		
		System.out.println(name);
		System.out.println(irum);
//		System.out.println(name == irum);	->	문자 배열이 문자열인 것은 맞지만 서로 비교할 수 는 없다.

		System.out.println(name == nick);
		System.out.println(name != nick);
	}

}
