package sec4;

public class Account {
	private int balance;
	final static int MIN_BALACE = 0;
	final static int MAX_BALACE = 1000000;
	
	public int getBalance() {
		return balance;
	}
	public void setBalance(int balance) {
		if(0 <= balance && balance <= 1000000){
			this.balance = 0 + balance;
		}
	}
	
	
}
