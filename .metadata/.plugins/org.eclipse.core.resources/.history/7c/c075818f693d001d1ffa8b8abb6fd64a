package sec2;

public class DmbPhoneEx {
	
	public static void main(String[] args) {
		CellPhone p1 = new CellPhone();
		CellPhone p2 = new DmbPhone();
//		DmbPhone p3 = new CellPhone();
		DmbPhone p4 = new DmbPhone();
		
		System.out.println(p1.CCOMPANY);
		System.out.println(p2.CCOMPANY);
		System.out.println(p4.CCOMPANY);
		
		p1.powerOn();
		p2.powerOn();
		p4.powerOn();
		
		//부모클래스로 부터 유도된 개체는 자식클래스로 부터 유도된 객체의 내용으로 변환될수 있음
		p1 = p4; //자동 형변환
		
	}

}
