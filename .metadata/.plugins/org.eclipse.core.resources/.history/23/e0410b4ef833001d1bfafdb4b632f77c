package sec3;

import java.util.Scanner;

public class Loop2 {
	public static void main(String[] args) {
		int jum[] = new int[5];
		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < jum.length; i++) {
			System.out.print((i+1)+"번째 사람 점수 : ");
			jum[i] = sc.nextInt();
		}
		
/*
		반복변수=초기값;
		while(조건식){
			반복실행문;
			증감식;
		}
*/
		int i = 0;
		int sum = 0;
		while(i < jum.length) {
			sum += jum[i];
			i++;
		}

		sc.close();
	}

}
