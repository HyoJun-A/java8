package sec1;

public class Var02 {
	int a=10, b=20;		//전역변수
	
	public static void main(String[] args){
		int a=100, b=200, c=300;	//지역변수
		System.out.println("a : " + a);
		// 지역변수 > 전역변수
		tot();
	}
	
	public static int tot(int f, int g){	//매개변수
		System.out.println("a+b="+(f+g));
		return f+g;
	}
}
