package sec1;

public class Oper4 {

	public static void main(String[] args) {
		int a=200, b=100, c=20, d=10;
		//비교연산자(==, !=, >, >=, <, <=) -> True or False의 결과가 산출됨
		System.out.println("a==b => "+(a==b));
		System.out.println("a!=b => "+(a!=b));
		System.out.println("a>b => "+(a>b));
		System.out.println("a>=b => "+(a>=b));
		System.out.println("a<b => "+(a<b));
		System.out.println("a<=b => "+(a<=b));
	}

}
